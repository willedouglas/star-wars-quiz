    "react-scripts": "1.1.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-promise-middleware": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
