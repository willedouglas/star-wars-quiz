{
  "name": "quiz-star-wars",
  "version": "1.0.0",
  "description": "Simple star wars quiz game developed 4 fun.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint --fix --ext .jsx,.js .",
    "build:_init": "rm -rf build/ && cp -R public/root ./build",
    "build": "npm run build:_init && NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "dev": "npm run build:_init && webpack-dev-server --config webpack.config.js --progress",
    "test": "jest"
  },
  "keywords": [
    "star",
    "wars",
    "quiz",
    "game"
  ],
  "author": "Douglas Wille <willedouglas10@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "16.2.0",
